https://leetcode.com/problems/car-pooling/

class Solution:
        
    def markTrip(self,l:List,trip:List) -> List:
        for i in range(trip[1],trip[2]):
            l[i] = l[i] + trip[0]
        return l
            
    def carPooling(self, trips: List[List[int]], capacity: int) -> bool:
        
        l = [0] * 1000
        
        for i in trips:
            l = self.markTrip(l,i)
            
        print(l)
            
        for i in l:
            if i > capacity:
                return False
        return True
            
